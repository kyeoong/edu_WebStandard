<script type="text/javascript">
	var a = new Array(4);
	document.write("a.length 1 = "+ a.length + "<br>");
	
	a[0] = 12;
	a[1] = 54;
	a[2] = 2;
	a[3] = 78;
	document.write("a.length 2 = "+ a.length + "<br>");
	document.write("<br> ===전체조회1=== <br>");
	for (var i = 0; i < a.length ; i++) {
		document.write("<br>a["+i+"] = "+a[i]);
	
	}
	
	document.write("<br><br> ===Java와 다른점1=== <br>");
	a[4] = 8;
	a[5] = 17;
	
	document.write("<br> ===전체조회2===");
	document.write("a.length 3 = "+ a.length + "<br>");
	for (var i = 0; i < a.length ; i++) {
		document.write("<br>a["+i+"] = "+a[i]);
	
	}
</script>
